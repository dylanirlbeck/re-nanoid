
bsc = ../lib/bsc.exe
bsc_no_open_flags = -absname -no-alias-deps -bs-no-version-header -bs-diagnose -bs-no-check-div-by-zero -bs-cross-module-opt -bs-package-name bs-platform -bs-package-output commonjs:lib/js  -bs-package-output es6:lib/es6  -nostdlib -nopervasives  -unsafe -w +50 -warn-error A  -bin-annot
bsc_flags = $bsc_no_open_flags -open Bs_stdlib_mini

rule cc
    command = $bsc -bs-cmi -bs-cmj $bsc_flags   -I runtime  $in
    description = $in -> $out
rule cc_cmi
    command = $bsc -bs-read-cmi -bs-cmi -bs-cmj $bsc_flags  -I runtime  $in
    description = $in -> $out    

build runtime/bs_stdlib_mini.cmi : cc runtime/bs_stdlib_mini.mli
    bsc_flags = -nostdlib -nopervasives
build runtime/js.cmj runtime/js.cmi : cc runtime/js.ml
    bsc_flags = $bsc_no_open_flags
